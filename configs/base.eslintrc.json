{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@typescript-eslint", "prettier", "import", "no-null"],
  "extends": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["node_modules", "*.d.ts"],
  "rules": {
    "@typescript-eslint/no-namespace": "off"
  }
}
